<UserControl x:Class="th.AdminibotModern.Pages.Settings.SettingsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Pages/Settings/Information/About.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="about" Source="/Pages/Settings/Information/About.xaml" />
                <mui:Link DisplayName="legal" Source="/Pages/Settings/Information/Legal.xaml" />
                <mui:Link DisplayName="third party" Source="/Pages/Settings/Information/ThirdParty.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>

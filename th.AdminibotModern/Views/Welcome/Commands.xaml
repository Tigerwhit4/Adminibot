<UserControl x:Class="th.AdminibotModern.Views.Welcome.CommandsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Views/Welcome/Commands/General.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="General" Source="/Views/Welcome/Commands/General.xaml" />
                <mui:Link DisplayName="Battletags" Source="/Views/Welcome/Commands/Battletags.xaml" />
                <mui:Link DisplayName="Currency" Source="/Views/Welcome/Commands/Currency.xaml" />
                <mui:Link DisplayName="Custom Commands" Source="/Views/Welcome/Commands/CustomCommands.xaml" />
                <mui:Link DisplayName="Moderation" Source="/Views/Welcome/Commands/Moderation.xaml" />
                <mui:Link DisplayName="Polls" Source="/Views/Welcome/Commands/Polls.xaml" />
                <mui:Link DisplayName="Raffles" Source="/Views/Welcome/Commands/Raffles.xaml" />
                <mui:Link DisplayName="Spam Protection" Source="/Views/Welcome/Commands/SpamProtection.xaml" />
                <mui:Link DisplayName="Time Watched" Source="/Views/Welcome/Commands/TimeWatched.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
